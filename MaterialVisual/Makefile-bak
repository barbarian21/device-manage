REPO?=harbor.uat.enflame.cc/library/devman-backend
TAG:=$(shell git rev-parse --abbrev-ref HEAD | sed -e 's/\//-/g')
COMPILEALL?=true
build:
	docker build . --build-arg COMPILEALL=$(COMPILEALL) --network host --no-cache --file Dockerfile --tag $(REPO):$(TAG)
push:
	docker push $(REPO):$(TAG)
all: build push
